<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="img-src">
            <values>
                <value id="sinch_media" type="host">media.stockinthechannel.com</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="sinch_quotes" type="host">*.quotes.stockinthechannel.co.uk</value>
            </values>
        </policy>
        <policy id="object-src">
            <values>
                <value id="object_src_self" type="host">self</value>
            </values>
        </policy>
        <policy id="base-uri">
            <values>
                <value id='base_uri_self' type='host'>self</value>
            </values>
        </policy>
        <policy id="frame-ancestors">
            <values>
                <value id='frame_ancestors_self' type='host'>self</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>